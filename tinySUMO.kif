(domain rangeSubclass 1 Function)
(domainSubclass rangeSubclass 2 Class)
(instance rangeSubclass AsymmetricRelation)
(instance rangeSubclass BinaryPredicate)

(=>
  (and
    (instance ?F Function)
    (rangeSubclass ?F ?C)
    (instance ?I (?F ?X)))
  (instance ?I ?C))

