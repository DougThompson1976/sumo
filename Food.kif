(subclass WholeMilk ProcessedMilk)
(documentation WholeMilk EnglishLanguage "&%WholeMilk is &%CowMilk which contains 3.25% to 5% of 
&%ButterFat. It is typically 3.25% of butter fat in countries such as Australia, Canada, USA.In the 
UK it is over 3.5%. [Wikipedia]")
(termFormat EnglishLanguage WholeMilk "whole milk")

(=>
  (and
    (instance ?M WholeMilk)
    (measure ?M (MeasureFn ?N ?U))
    (instance ?U UnitOfVolume))
  (exists (?BF ?N2)
    (and
      (instance ?BF ButterFat)
      (part ?BF ?M)
      (not
        (exists (?F2)
          (and
            (instance ?F2 ButterFat)
            (not
              (equal ?BF ?F2))
            (part ?F2 ?M))))
      (measure ?BF (MeasureFn ?N2 ?U))
      (greaterThanOrEqualTo ?N2 (MultiplicationFn 0.0325 ?N))
      (lessThanOrEqualTo ?N2 (MultiplicationFn 0.05 ?N)))))
      
