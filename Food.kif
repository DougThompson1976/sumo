(subclass Curdling StateChange)
(documentation Curdling EnglishLanguage "&%Curdling in is the breaking of an &%Emulsion or &%Colloid 
into large parts of different composition through the physio-chemical processes of flocculation, creaming, 
and coalescence. Curdling occurs naturally but it can also be intentially. It is purposeful in the 
production of cheese curd and tofu; undesirable in the production of a sauce, cheese fondue or a custard.[Wikipedia]")
(termFormat EnglishLanguage Curdling "curdling")

(=>
  (instance ?C Curdling)
  (exists (?X)
    (and
      (patient ?C ?X)  
      (holdsDuring
        (BeginFn (WhenFn ?C))
        (instance ?X Colloid))
      (holdsDuring
        (EndFn (WhenFn ?C))
        (not
          (instance ?X Colloid))))))
    
(=>
  (and
    (instance ?C Curdling)
    (patient ?C ?X)
    (instance ?X Colloid))      
  (exists (?S ?A)
    (and
      (instance ?S Separating)
      (subProcess ?S ?C)
      (patient ?S ?X)
      (result ?S ?A)
      (piece ?A ?X)
      (instance ?A Substance))))
      
(subclass CurdlingMilk Curdling)
(documentation CurdlingMilk EnglishLanguage "&%CurdlingMilk is the &%Process in which the pH of the milk decreases 
and becomes more acidic. Independently floating casein molecules attract one another, forming lumps that float in
a translucent whey.[Wikipedia]")
(termFormat EnglishLanguage CurdlingMilk "curdling milk")

(=>
  (instance ?C CurdlingMilk)
  (exists (?M)
    (and
      (instance ?M Milk)
      (patient ?C ?M))))

(=>
  (and
    (instance ?C CurdlingMilk)
    (instance ?M Milk)
    (patient ?C ?M))
  (exists (?PH ?D ?H ?L)
    (and
      (measure ?M
          (MeasureFn ?PH PHValue))     
      (instance ?D Decreasing)
      (subProcess ?D ?C)
      (patient ?D ?PH)
      (holdsDuring
        (BeginFn
          (WhenFn ?D))
        (equal ?PH ?H)
        (approximateValue ?H 6.5))
      (holdsDuring
        (EndFn
          (WhenFn ?D))
        (equal ?PH ?L)
        (approximateValue ?L 4.5))))) 
        
(=>
  (instance ?CM CurdlingMilk)
  (exists (?C ?W)
    (and
      (result ?CM ?C)
      (result ?CM ?W)
      (instance ?C Curd)
      (instance ?W Whey))))
     
