    (instance ?M Measuring)
    (instrument ?M ?PO)
    (agent ?M ?A)
    (patient ?M ?P)
    (instance ?PO PulseOximeter))
  (exists (?N)
    (knows ?A
      (oxygenSaturation ?P ?N))))

(=>
